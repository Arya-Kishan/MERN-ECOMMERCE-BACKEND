{
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "dist/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/user",
            "dest": "index.js"
        },
        {
            "src": "/user/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/product",
            "dest": "index.js"
        },
        {
            "src": "/product/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/cart",
            "dest": "index.js"
        },
        {
            "src": "/cart/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/order",
            "dest": "index.js"
        },
        {
            "src": "/order/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/categories",
            "dest": "index.js"
        },
        {
            "src": "/categories/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/brands",
            "dest": "index.js"
        },
        {
            "src": "/review",
            "dest": "index.js"
        },
        {
            "src": "/create-checkout-session",
            "dest": "index.js"
        },
        {
            "src": "/count/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/wishlist/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/",
            "dest": "dist/index.html"
        },
        {
            "src": "/(.+)",
            "dest": "dist/$1"
        }
    ]
}